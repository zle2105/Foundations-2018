append('circle')\n    .attr('class', 'city')\n    .attr('r', 1.5)\n    .attr('fill', d => colorScale(d.population))\n    .attr('transform', d => {\n      let coords = projection([d.lng, d.lat])\n      return `translate(${coords})`\n    })\n}\n"},"lineCount":null}},"hash":"ef1903d362a8e796f058e1a274aad96f","cacheData":{"env":{}}}